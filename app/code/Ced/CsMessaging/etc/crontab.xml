<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">

    <!-- cron for customer emails -->
    <group id="default">
        <job name="csmessaging_customer_vendor_mails" instance="Ced\CsMessaging\Cron\SendMail" method="execute">
            <schedule>*/5 * * * *</schedule>
        </job>
    </group>
</config>